#include <iostream>
#include <string>

using namespace std;

class Klasa{
    public:
        int x;
        int y;
        int id;
        static int ID;
        
        Klasa(int zmienna1,int zmienna2){
            x = zmienna1;
            y = zmienna2;
            id = ID++;
            cout << "Obiekt utworzony" << endl;
            cout << "Konstruktor parametryczny uruchomiony" << endl;
            cout << "ID nowego obiektu ma nr: " << ID << endl;
            
        }
        
        Klasa(Klasa &kopia){
            x = kopia.x;
            y = kopia.y;
            
            
            id = ID++;
            
            cout << "Konstruktor kopiujÄ…cy uruchomiony" << endl;
            
            //cout << "Utworzono kopie obiektu: " << kopia.ID << endl; 
            cout << "ID nowego obiektu ma nr: " << ID << endl;
        }
};
int Klasa::ID = 0;
int main()
{
    Klasa k1(1,1);
    cout << endl;
    Klasa k2(k1);
    
    Klasa k3(k1);
    Klasa k4(k2);
}
